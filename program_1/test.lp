#include "instances/network_NY_0.lp".
edge(E) :- nas(_,E,_,_). 
agent(D) :- nas(D,_,_,_). 
step(D,S) :- nas(D,_,_,S). 


total(T, E) :- edge(E), T = #sum{W,D,X : nas(D,E,W,X)}.

% empty flights
wasted(F) :- F = #count{(L,L'),D,X : nas(D,(L,L'), 0, X), L!=L'}.
flight_time_need(D, TD) :- agent(D), TD = #sum{T, X : flight_time(E, T), nas(D, E, _, X)}.
charge_time_need(D, TD) :- agent(D), TD = #sum{T, X : charge_time((L,L1), T), nas(D, (L,L1), _, X), nas(D, (_,L), _, X-1)}.

total_time_need(D, FT + CT) :- agent(D), flight_time_need(D, FT), charge_time_need(D, CT).

